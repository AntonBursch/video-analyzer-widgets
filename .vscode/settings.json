{
    "files.eol": "\n",
    "cSpell.words": ["RVX", "emsg", "mediafront", "npmrc", "starttime", "vsts", "infocus", "outfocus", "avarvx"],
    "cSpell.ignorePaths": ["**/package-lock.json", "**/node_modules/**", ".vscode", "**/webpack.config.js"],
    "typescript.tsdk": "node_modules\\typescript\\lib",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
